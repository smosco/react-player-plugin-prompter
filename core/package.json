{
  "name": "react-player-plugin-prompter",
  "description": "A React Player plugin specialized for subtitle handling",
  "version": "0.0.5",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "yarn@4.3.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smosco/react-player-plugin-prompter.git"
  },
  "keywords": [
    "react",
    "react-player",
    "subtitle",
    "video",
    "plugin"
  ],
  "contributors": [
    "smosco",
    "godhyzzang"
  ],
  "type": "module",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "node ./esbuild.config.js",
    "build:dts": "tsc --emitDeclarationOnly --outDir dist",
    "dev": "node ./esbuild.config.js --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "esbuild": "^0.24.0",
    "esbuild-scss-modules-plugin": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.79.4",
    "typescript": "5.5.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0",
    "react-dom": ">=16.8.0 <19.0.0"
  }
}
